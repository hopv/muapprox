/* 変換結果はiwayama solverを用いないと解けない */
%RANK
br -> 2.
s -> 1.
e -> 0.

%GRAMMAR
S -> F One Three.
F (x: unit -> unit) (y: unit -> unit) -> br (x e) (F y (Plus x y)).
Plus (x: unit -> unit) (y: unit -> unit) (z: unit) -> x(y z).
One (z: unit) -> s(z).
Three (z: unit) -> s(s(s(z))).

%TRANSITION
q0 br -> (1,odd) /\ (2,q0).
q0 s -> false.
q0 e -> false.
odd br -> false.
odd s -> (1,even).
odd e -> false.
even br -> false.
even s -> (1,odd).
even e -> true.

%PRIORITY
q0 -> 0.
odd -> 0.
even -> 0.
