Main n k g =u
  Check g (\b.
    (b =  0 => k 0) /\
    (b != 0 => Main n k (Pred g))
  ).

Mult acc f g k =u
  Check f (\b.
    (b = 0 => k acc) /\
    (b = 1 => Mult (Add acc g) (Pred f) g k)
  ).

Add f g k =u f (\fx. g (\gx. k (fx + gx))).

Pred f k =u f (\r. k (r - 1)).

Check f k =u f (\n. (n <= 0 => k 0) /\ (n > 0 => k 1)).
